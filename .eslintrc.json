{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "comma-dangle": ["error", "never"],
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off",
    "arrow-parens": "off"
  }
}
